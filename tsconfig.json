{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",

        "rootDir": "src",
        "outDir": "dist",

        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "strictNullChecks": true
    }
}
